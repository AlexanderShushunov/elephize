{
  "src": "src/**/*.tsx",
  "outDir": "build",
  "output": "bootstrap.php",
  "quiet": false,
  "bail": "none",
  "dump": false,
  "noZap": false,
  "baseDir": ".",
  "tsPaths": {
    "#aliasedFolder/*": ["src/aliased/*"]
  },
  "aliases": {
    "/components/iso_palette": "Components",
    "/utils": "Utils"
  },
  "customGlobals": {
    "getLang": "CustomBackendService::getLang"
  }
}